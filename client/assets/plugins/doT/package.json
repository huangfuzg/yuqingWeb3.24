    "node >=0.2.6"
  ],
  "license": "MIT",
  "scripts": {
    "test": "nyc mocha"
  },
  "dependencies": {},
  "devDependencies": {
    "commander": "*",
    "coveralls": "^2.11.14",
    "jshint": "*",
    "mkdirp": "*",
    "mocha": "*",
    "nyc": "^8.3.2",
    "pre-commit": "^1.1.3",
    "uglify-js": "*"
  },
  "nyc": {
